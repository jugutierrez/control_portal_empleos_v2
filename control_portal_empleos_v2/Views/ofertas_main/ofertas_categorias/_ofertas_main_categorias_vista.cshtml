@model IEnumerable<control_portal_empleos_v2.Models.ofertas_categorias>

@if (Model.Count() != 0)
{
  foreach (var item in Model)
 {
<h3>
   
    <span class="label label-info item-select">


       @Html.DisplayFor(model => item.categorias.nombre_categoria)

@if ((bool)ViewBag.editar_ofertas == true)
{
        <i id="btn1" class="btn1 fa fa-close" data-url="/ofertas_main/seleccion_elimina_categoria/@Html.DisplayFor(model => item.id_oferta_categoria)" data-toggle="modal" data-target="#mdsoporte"></i>

}

    </span>
</h3>
 }
}
else
{
<ul  class="list-group">
    <li  class="list-group-item text-center">
         no se ha ingresado ninguna Categoria
    </li>
</ul>
}