@model IEnumerable<control_portal_empleos_v2.Models.ofertas_documentos>

    @if (Model.Count() != 0)
    {
        foreach (var item in Model)
        {
            <h3>

                <span class="label label-info item-select">

                    <a id="btn1" class="btn1" data-url="/ofertas_main/detalle_documentos_ofertas/@Html.DisplayFor(model => item.id_oferta_documento)" data-toggle="modal" data-target="#mdsoporte">

                        @Html.DisplayFor(model => item.nombre_documento_oferta) (@Html.DisplayFor(model => item.relevancia_documentos.nombre_relevancia_documento))
                    </a>
@if ((bool)ViewBag.editar_ofertas == true)
{
                    <i id="btn1" class="btn1 fa fa-close" data-url="/ofertas_main/seleccion_elimina_documento/@Html.DisplayFor(model => item.id_oferta_documento)" data-toggle="modal" data-target="#mdsoporte"></i>

}

                </span>
            </h3>
        }
    }
    else
    {
        <ul class="list-group">
            <li class="list-group-item text-center">
                no se ha ingresado ningun documento
            </li>
        </ul>
    }
